<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <add key="config" value="False" />
    <add key="ip-adress" value="192.168.2.100" />
    <add key="port" value="4210" />
    <add key="refresh" value="100" />
    <add key="top" value="True" />
